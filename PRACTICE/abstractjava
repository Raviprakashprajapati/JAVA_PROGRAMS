
abstract class OperatingSystem {
    abstract void op();
    abstract void op1();
}
abstract class Computer extends OperatingSystem {
    int a = 789;
    abstract void meth1();
    abstract void meth2();
}




class Laptop extends Computer{
    
    void meth1(){
        System.out.println("Laptop meht1 "+a);
    }
    void meth2(){
        System.out.println("Laptop meht2");
    }
    void op(){
        System.out.println("Operating system op");
    }
    void op1(){
        System.out.println("Operating system op1");

    }
}


public class abstract6 {
    public static void main(String[] args) {
        

        // Laptop asus = new Laptop();
        // asus.meth1();
        // asus.a = 456;
        // asus.meth1();


        // Computer c = new Laptop();
        // c.op();


    }
    
}
